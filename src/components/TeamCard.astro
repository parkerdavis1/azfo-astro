---
const {
    slug,
    body,
    data: { name, title, blurb },
} = Astro.props.data;

const { Content } = await Astro.props.data.render();

const birdEmojis = ['🕊️', '🦅', '🦆', '🦜', '🐥'];
---

<details>
    <summary>{name} - {title}</summary>
    <p>
        {blurb}
    </p>
    <Content />
</details>
